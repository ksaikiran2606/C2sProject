{
  "cli": {
    "version": ">= 5.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      },
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "https://c2sproject-production.up.railway.app/api",
        "EXPO_PUBLIC_WS_BASE_URL": "wss://c2sproject-production.up.railway.app"
      }
    },
    "production": {
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      },
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "https://c2sproject-production.up.railway.app/api",
        "EXPO_PUBLIC_WS_BASE_URL": "wss://c2sproject-production.up.railway.app"
      }
    }
  },
  "submit": {
    "production": {}
  }
}

